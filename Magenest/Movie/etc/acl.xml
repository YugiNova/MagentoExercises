<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation=
    "urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magenest_Movie::movie_menu" title="Movie" sortOrder="1">
                    <resource id="Magenest_Movie::movie_index" title="Movie index" sortOrder="1"/>
                    <resource id="Magenest_Movie::actor_index" title="Actor index" sortOrder="2"/>
                    <resource id="Magenest_Movie::director_index" title="Director index" sortOrder="3"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource
                                id="Magenest_Movie::movie_section"
                                title="Movie Config"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
